<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         metadata-complete="false">

	<servlet>
		<servlet-name>voiture</servlet-name>
		<servlet-class>covoiturage.VoitureServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>updateCalendrier</servlet-name>
		<servlet-class>covoiturage.UpdateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>voiture</servlet-name>
		<url-pattern>/voiture</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>updateCalendrier</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
</web-app>
